	public static void twoWheeler()
	{
		Scanner m=new Scanner(System.in);
		System.out.println("==========================================================================================");
	ArrayList lk=new ArrayList();
	lk.add("1 for Scooter ");
	lk.add("2 for bike ");
	lk.add("3 for Bicycle");
	lk.add("4 for Compact Scooter");
	lk.add("5 for Hoverboard");
	System.out.println(lk);
	System.out.println("==========================================================================================");
	int ch=m.nextInt();
	
	if(ch<6)
	{
		switch(ch)
		{
		case 1: scooter();
		break;
		case 2: bike();
		break;
		case 3: bicycle();
		break;
		case 4: compactScooter();
		break;
		case 5: hoverboard();
		break;
		default : System.out.println("Please choose right key");
		break;
		}
	}
	else if(ch>5)
	{
		System.out.println("please choose correct option");
		System.out.println("===========================================================");
		System.out.println(lk);
		System.out.println("===========================================================");
		int n=m.nextInt();
		switch(n)
		{
		case 1: scooter();
		break;
		case 2: bike();
		break;
		case 3: bicycle();
		break;
		case 4: compactScooter();
		break;
		case 5: hoverboard();
		break;
		default : System.out.println("Please choose right key");
		break;
		}

	}
	else if(ch>5)
	{
		System.out.println("please choose correct option");
		System.out.println("===========================================================");
		System.out.println(lk);
		System.out.println("===========================================================");
		int n=m.nextInt();
		switch(n)
		{
		case 1: scooter();
		break;
		case 2: bike();
		break;
		case 3: bicycle();
		break;
		case 4: compactScooter();
		break;
		case 5: hoverboard();
		break;
		default : System.out.println("Please choose right key");
		break;
		}

	}
	}
	
