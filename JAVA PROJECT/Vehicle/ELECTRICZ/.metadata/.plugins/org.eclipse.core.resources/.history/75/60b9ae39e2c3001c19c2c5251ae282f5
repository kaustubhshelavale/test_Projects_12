	public static void bounceInfinity()
	{
		Scanner m=new Scanner(System.in);
		System.out.println("==========================================================================================");
	ArrayList lk=new ArrayList();
	lk.add("1 for Bounce Infinity E1");
	System.out.println(lk);
	System.out.println("==========================================================================================");
	int ch=m.nextInt();
	if(ch<2)
	{
		switch(ch)
		{
		case 1:e1();
		break;
		default :System.out.println("please choose correct option");
		break;
		}
	}
	}
