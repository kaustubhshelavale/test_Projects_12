package XMoto;

public class Xmoto
{
	public static void main(String[] args) 
	{
		
	}	
}
class Bicycle extends Xmoto
{
	public static void main(String[] args) 
	{
		ChooseCompany(1);
		Hercules(4);
	}
	public static void ChooseCompany(int a)
	{
		switch(a)
		{
		case 1: Bicycle.Hercules(a);
		break;
		case 2: System.out.println("H");
		break;
		case 3: System.out.println("Ho");
		break;
		case 4: System.out.println("5difo");
		break;
		case 5: System.out.println("eoisufdh");
		break;
		default:
		break;
		}
	}
	public static void Hercules(int a)
	{
		switch(a)
		{
		case 1: System.out.println("1");
		break;
		case 2: System.out.println("osiuyfghw");
		break;
		case 3: System.out.println("oiuyrdeoso");
		break;
		case 4: System.out.println("oiuyrhdgxo");
		break;
		case 5: System.out.println("Hiusf");
		break;
		default:
		break;
	}

	}
	public static void Roadeo(int a)
	{
		switch(a)
		{
		case 1: System.out.println("Hero");
		break;
		case 2: System.out.println("Hero");
		break;
		case 3: System.out.println("Hero");
		break;
		case 4: System.out.println("Hero");
		break;
		case 5: System.out.println("Hero");
		break;
		default:
		break;
	}

	}
}
