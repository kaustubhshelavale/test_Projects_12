package XMoto;

public class Xmoto
{
	static
	{
		System.out.println("tumarichit");
	}
	{
		System.out.println("chut");
	}
	
	
	public static void main(String[] args) 
	{
		Motorcycle p=new Motorcycle();
		System.out.println("Hi");
	}	

}
